{
    "name": "lanin/laravel-api-debugger",
    "description": "Easily debug your JSON API.",
    "keywords": ["laravel", "framework", "api", "debug", "debugger", "json", "dump", "queries", "log"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Lanin",
            "email": "max@lanin.me",
            "homepage": "http://lanin.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7.6"
    },
    "autoload": {
        "psr-4": {
           "Lanin\\ApiDebugger\\": "src/"
        },
        "classmap": [
            "tests/DebuggerTest.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
